{
  "FireBase": {
    "ApiKey": "${FIREBASE_API_KEY}%", // Public API Key
    "AuthEnabled": true,
    "ClientName": "FireBase",
    "DatabaseUrl": "${FIREBASE_DB_URL}%",
    "ServiceAccountCredentials": "${SERVICE_ACCOUNT}$",
    "TokenRenewalMinutes": 50 // Expires after an hour, so renew it earlier
  }
}
